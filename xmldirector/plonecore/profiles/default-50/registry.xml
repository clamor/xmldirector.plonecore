<?xml version="1.0" ?>
<registry 
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone">
    <records interface="xmldirector.plonecore.interfaces.IWebdavSettings" />

    <records prefix="plone.resources/xmldirector.plonecore"
          interface="Products.CMFPlone.interfaces.IResourceRegistry">
        <value key="js">++resource++xmldirector.plonecore/local.js</value>
        <value key="css">
            <element>++resource++xmldirector.plonecore/styles.css</element>
        </value>
    </records>

    <records prefix="plone.bundles/plone-legacy"
         interface='Products.CMFPlone.interfaces.IBundleRegistry'>
        <value key="resources">
            <element>xmldirector.plonecore</element>
        </value>
    </records>
</registry>
